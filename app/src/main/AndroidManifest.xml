<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moodtracker">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FriendDetailActivity"></activity>
        <activity android:name=".FriendMoodHistoryActivity" />
        <activity android:name=".SearchResultActivity" />
        <activity android:name=".AcceptFriendActivity" />
        <activity android:name=".FriendActivity" />
        <activity android:name=".CustomMoodHistory" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MoodActivity" />
        <activity android:name=".AddActivity" />
        <activity android:name=".MoodHistoryActivity" />
        <activity android:name=".OptionActivity" />
        <activity android:name=".EditActivity" />
        <activity android:name=".ViewPictureActivity" />
        <activity android:name=".RegisterActivity" />
    </application>

</manifest>